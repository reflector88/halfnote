<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

   <Color x:Key="MainUIColor">#161616</Color>
   <Color x:Key="EditorColor">#1F1F1F</Color>
   <Color x:Key="PreviewerColor">#282828</Color>
   <Color x:Key="BorderColor">Black</Color>
   <Color x:Key="SidebarColor">#0F0F0F</Color>
   <Color x:Key="HighlightColor">Orchid</Color>
   <Color x:Key="ContrastColor">#F6F6F6</Color>
   <Color x:Key="ListBoxHighlightColor">#181818</Color>
   <Color x:Key="ListBoxSelectionColor">#282828</Color>
   <Color x:Key="TableEvenColor">#161b22</Color>
   <Color x:Key="TableBorderColor">#30363d</Color>
   <Color x:Key="TitleBarElementColor">#282828</Color>
   <FontFamily x:Key="MonospaceFontStack">--fontStack-monospace, ui-monospace, SFMono-Regular, SF Mono, Menlo, Consolas, Liberation Mono, monospace</FontFamily>
   <FontFamily x:Key="SystemFontStack">-apple-system, BlinkMacSystemFont, Segoe UI, Roboto, Oxygen-Sans, Ubuntu, Cantarell, Helvetica, Arial, sans-serif</FontFamily>

</ResourceDictionary>
